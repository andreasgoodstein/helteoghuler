; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="HelteOgHulerClient"
run/main_scene="res://scenes/LoginMenuScene.tscn"
config/icon="res://assets/icon.png"

[autoload]

Settings="*res://scripts/autoloads/Settings.cs"
GlobalGameState="*res://scripts/autoloads/GlobalGameState.cs"
Server="res://scripts/autoloads/Server.cs"

[display]

window/size/height=720

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://themes/theme.tres"

[importer_defaults]

csv_translation={
"compress": false,
"delimiter": 1
}

[locale]

translations=PoolStringArray( "res://translations/translations.en.translation" )
translation_remaps={
}
locale_filter=[ 0, [ "da", "en" ] ]

[mono]

project/assembly_name="HelteOgHulerClient"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
